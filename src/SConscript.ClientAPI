# vim: ft=python

Import('env')

dir = 'ClientAPI'

envCopy = env.Clone()
envCopy.Append(
                CPPPATH = '.'
              )

sourceTargets = [ 'API.cpp',
                  'APIHelpers.cpp' ]

targets = []
for source in sourceTargets:
  targets.append(dir + '/' + source)

library = envCopy.Library('ClientAPI',targets)

Return( 'library' )
