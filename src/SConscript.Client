# vim: ft=python

Import('env')

dir = 'Client'

envCopy = env.Clone()
envCopy.Append(
                CPPPATH = '.'
              )

sourceTargets = [ # nothing to build yet
                  ]

targets = []
for source in sourceTargets:
  targets.append(dir + '/' + source)

program = envCopy.Program(target = '../bin/Client',
                          source = targets)

Return( 'program' )
